{
    "name": "deh4eg/omnipay-kevin",
    "description": "Kevin gateway based on omnipay library",
    "type": "library",
    "keywords": [
        "gateway",
        "omnipay",
        "kevin",
        "payment",
        "merchant"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Denis Smolakov",
            "email": "denissmolakovs@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Omnipay\\Kevin\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "require": {
        "php": "^7.2",
        "omnipay/common": "^3.0"
    },
    "require-dev": {
        "omnipay/tests": "^3.1",
        "phpro/grumphp": "^0.19.1",
        "phpstan/phpstan": "^0.12.82",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp": true
        }
    }
}
